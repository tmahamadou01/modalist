<?php

namespace ModalistBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * WpProducts
 */
class WpProducts
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $productRef;

    /**
     * @var string
     */
    private $type;

    /**
     * @var string
     */
    private $marque;

    /**
     * @var integer
     */
    private $stock;

    /**
     * @var string
     */
    private $urlPhoto1;

    /**
     * @var string
     */
    private $urlPhoto2;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set productRef
     *
     * @param string $productRef
     * @return WpProducts
     */
    public function setProductRef($productRef)
    {
        $this->productRef = $productRef;

        return $this;
    }

    /**
     * Get productRef
     *
     * @return string 
     */
    public function getProductRef()
    {
        return $this->productRef;
    }

    /**
     * Set type
     *
     * @param string $type
     * @return WpProducts
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return string 
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Set marque
     *
     * @param string $marque
     * @return WpProducts
     */
    public function setMarque($marque)
    {
        $this->marque = $marque;

        return $this;
    }

    /**
     * Get marque
     *
     * @return string 
     */
    public function getMarque()
    {
        return $this->marque;
    }

    /**
     * Set stock
     *
     * @param integer $stock
     * @return WpProducts
     */
    public function setStock($stock)
    {
        $this->stock = $stock;

        return $this;
    }

    /**
     * Get stock
     *
     * @return integer 
     */
    public function getStock()
    {
        return $this->stock;
    }

    /**
     * Set urlPhoto1
     *
     * @param string $urlPhoto1
     * @return WpProducts
     */
    public function setUrlPhoto1($urlPhoto1)
    {
        $this->urlPhoto1 = $urlPhoto1;

        return $this;
    }

    /**
     * Get urlPhoto1
     *
     * @return string 
     */
    public function getUrlPhoto1()
    {
        return $this->urlPhoto1;
    }

    /**
     * Set urlPhoto2
     *
     * @param string $urlPhoto2
     * @return WpProducts
     */
    public function setUrlPhoto2($urlPhoto2)
    {
        $this->urlPhoto2 = $urlPhoto2;

        return $this;
    }

    /**
     * Get urlPhoto2
     *
     * @return string 
     */
    public function getUrlPhoto2()
    {
        return $this->urlPhoto2;
    }
}
